{
    "name": "enspyred-theme",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "watch": "vite build --watch",
        "build": "vite build",
        "clean": "rm -rf build",
        "prebuild": "npm run clean",
        "sync-version": "node sync-version.js",
        "prepackage": "npm run sync-version",
        "package": "npm run build && npm run package:create",
        "package:create": "rm -rf ../../../dist/enspyred-theme && mkdir -p ../../../dist/enspyred-theme && rsync -av --exclude-from=.distignore . ../../../dist/enspyred-theme/ && cd ../../../dist && zip -r enspyred-theme.zip enspyred-theme && find enspyred-theme -name '.DS_Store' -delete"
    },
    "dependencies": {
        "prop-types": "^15.8.1",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "styled-components": "^6.1.17"
    },
    "devDependencies": {
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "@vitejs/plugin-react": "^4.3.4",
        "globals": "^16.0.0",
        "vite": "^6.3.1"
    },
    "author": "Mitch Gohman",
    "license": "ISC",
    "browserslist": [
        "defaults",
        "not IE 11",
        "maintained node versions"
    ]
}